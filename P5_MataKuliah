/**
 * Write a description of class MataKuliah here.
 *
 * @author yasmina
 * @version 23-09-2025
 */

class MataKuliah {
    private String kode;
    private String nama;
    private Dosen dosen;

    public MataKuliah(String kode, String nama, Dosen dosen) {
        this.kode = kode;
        this.nama = nama;
        this.dosen = dosen;
        dosen.tambahMataKuliah(this);
    }

    public String getKode() {
        return kode;
    }

    public String getNama() {
        return nama;
    }

    public Dosen getDosen() {
        return dosen;
    }
}
